  "bugs": {
    "url": "https://github.com/YummyOreo/document-site/issues"
  },
  "homepage": "https://github.com/YummyOreo/document-site#readme",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
